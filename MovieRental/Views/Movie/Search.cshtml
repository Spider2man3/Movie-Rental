@model MovieRental.Models.MovieModel
@{
    ViewBag.Title = "Search - ChillFlix";
    ViewBag.Logged = Model.LoggedIn;
    if (Model.User != null)
    {
        ViewBag.User = Model.User.Email;
    }
}


<div class="container">
    @foreach (var item in Model.Movies)
    {
        <a href="@Url.Action("Movie", "Movie", new { movie = item.Title})"><img src="~/pythonfiles/@item.Picture" height="50%" width="20%" alt="@item.Title"/></a>
    }
</div>